{
  "name": "@aklinker1/mcp-server-sdk",
  "version": "0.1.1",
  "description": "Server-side APIs for creating MCP server-side fetch functions",
  "packageManager": "bun@1.3.5",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch dev.ts",
    "dev:mcp": "bunx @modelcontextprotocol/inspector@latest --transport http --server-url http://localhost:3000",
    "build": "tsdown src/index.ts --dts",
    "prepack": "bun run build"
  },
  "devDependencies": {
    "@aklinker1/check": "^2.2.0",
    "@types/bun": "latest",
    "@types/uri-template-lite": "^19.12.4",
    "@typescript/native-preview": "^7.0.0-dev.20260114.1",
    "oxlint": "^1.39.0",
    "publint": "^0.3.16",
    "prettier": "^3.7.4",
    "tsdown": "^0.20.0-beta.3",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.1.0",
    "uri-template-lite": "^23.4.0"
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    "types": "./dist/index.d.mts",
    "default": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://jsr.io/@aklinker1/mcp-server-sdk",
  "repository": {
    "url": "https://github.com/aklinker1/mcp-server-sdk"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
